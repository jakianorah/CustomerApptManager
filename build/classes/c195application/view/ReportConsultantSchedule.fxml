<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="root" stylesheets="@style.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="c195application.view.ReportConsultantScheduleController">
    <children>
		<BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<bottom>
				<HBox alignment="CENTER" BorderPane.alignment="CENTER">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</HBox>
			</bottom>
			<top>
				<HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
					<children>
                                            <Label styleClass="label-title" text="Consultant Schedule" >
                                            </Label>
						<ComboBox fx:id="consultantComboBox" prefWidth="150.0" promptText="selectConsultant" />
					</children>
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</HBox>
			</top>
			<center>
				<TextArea fx:id="scheduleTextArea" BorderPane.alignment="CENTER">
					<BorderPane.margin>
						<Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
					</BorderPane.margin>
				</TextArea>
			</center>
		</BorderPane>
	</children>
</AnchorPane>
